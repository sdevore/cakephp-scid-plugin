{
  "name": "sdevore/cakephp-scid-plugin",
  "type": "cakephp-plugin",
  "description": "Scid plugin for CakePHP",
  "license": "MIT",
  "require": {
    "php": ">=7.0.0",
    "cakephp/cakephp": ">=3.3.2 <4.0.0",
    "cebe/markdown": "*",
    "friendsofcake/bootstrap-ui": "dev-develop",
    "fzaninotto/faker": "^1.7",
    "gourmet/email": "~3.0",
    "moneyphp/money": "^3.1.0",
    "picqer/php-barcode-generator": "^0.2.2",
    "tijsverkoyen/css-to-inline-styles": "^2.2",
    "twbs/bootstrap": "^4.1.0",
    "oomphinc/composer-installers-extender": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "suggest": {
    "dereuromark/cakephp-ide-helper": "annotate classes and views to work better with autocomplete in IDE",
    "lavary/crunz": "useful for scheduling"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Scid\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Scid\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  },
  "extra": {
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "webroot/assets/{$vendor}/{$name}/": [
        "type:bower-asset",
        "type:npm-asset"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
